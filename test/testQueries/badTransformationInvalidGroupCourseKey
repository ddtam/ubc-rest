{
    "WHERE": {
        "AND": [{
            "IS": {
                "courses_dept": "*SP*"
            }
        }, {
            "GT": {
                "courses_year": 2015
            }
        }]
    },
    "OPTIONS": {
        "COLUMNS": [
            "courses_dept",
            "maxAvg"
        ],
        "ORDER": {
            "dir": "DOWN",
            "keys": ["maxAvg"]
        }
    },
    "TRANSFORMATIONS": {
        "GROUP": ["courses_dept"],
        "APPLY": [{
            "minAvg": {
                "MIN": "courses_avg"
            }
        }]
    }
}