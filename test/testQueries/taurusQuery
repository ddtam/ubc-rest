{
    "WHERE": {
        "AND": [{
            "IS": {
                "courses_dept": "CPSC"
            }
        }, {
            "GT": {
                "courses_audit": -1
            }
        }]
    },
    "OPTIONS": {
        "COLUMNS": [
            "courses_dept",
            "courses_id",
            "avgAvg"
        ],
        "ORDER": {
            "dir": "DOWN",
            "keys": ["avgAvg"]
        }
    },
    "TRANSFORMATIONS": {
        "GROUP": ["courses_id"],
        "APPLY": [{
            "avgAvg": {
                "AVG": "courses_avg"
            }
        }]
    }
}